<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="10.0" minWidth="10.0" prefHeight="136.0" prefWidth="404.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fhnw.lernstickwelcome.fxmlcontroller.standard.HwStatisticController">
   <children>
      <Label maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="384.0" text="%welcomeApplicationHardwareStatistic.informationText" wrapText="true" VBox.vgrow="ALWAYS" />
      <ButtonBar>
        <buttons>
          <Button fx:id="yesButton" defaultButton="true" mnemonicParsing="false" prefHeight="26.0" text="%welcomeApplicationInfodialog.ok" ButtonBar.buttonData="RIGHT" />
          <Button fx:id="noButton" cancelButton="true" minWidth="31.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="363.0" text="%welcomeApplicationInfodialog.cancel" ButtonBar.buttonData="RIGHT" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
